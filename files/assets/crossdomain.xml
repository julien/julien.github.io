<?xml version="1.0"?>
<!DOCTYPE cross-domain-policy
  SYSTEM "http://www.macromedia.com/xml/dtds/cross-domain-policy.dtd">
<cross-domain-policy
	xsi:noNamespaceSchemaLocation="http://www.adobe.com/xml/schemas/PolicyFile.xsd">


	<allow-access-from domain="*.punkscum.org" secure="true" />
	<allow-access-from domain="*.google.com"   secure="true" />
	<allow-access-from domain="*.ytimg.com"    secure="true" />
	<allow-access-from domain="*.youtube.com"  secure="true" />
    <allow-access-from domain="*.localhost"    secure="true" />
	<site-control permitted-cross-domain-policies="master-only"/>

	<allow-http-request-headers-from
		domain="*.youtube.com"
		headers="*"
		secure="true"/>

	</cross-domain-policy>